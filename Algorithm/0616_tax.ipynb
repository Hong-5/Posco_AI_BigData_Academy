    "# 50000\n",
    "# 10000\n",
    "# 5000\n",
    "# 1000\n",
    "# 500\n",
    "# 100\n",
    "t = int(input())\n",
    "price_list = [50000, 10000, 5000, 1000, 500, 100]\n",
    "\n",
    "for _ in range(t):\n",
    "    n = int(input())\n",
    "    cnt = 0\n",
    "    for i in price_list:\n",
    "        cnt += n // i\n",
    "        n = n % i\n",
    "    print(cnt)"
   ]
