   ],
   "source": [
    "import heapq\n",
    "\n",
    "t = int(input())\n",
    "\n",
    "for _ in range(t):\n",
    "    n = int(input())\n",
    "    deque_list = []\n",
    "    for _ in range(n):\n",
    "        qry = int(input())\n",
    "        if qry == -1:\n",
    "            print(heapq.heappop(deque_list))\n",
    "        else:\n",
    "            heapq.heappush(deque_list, qry)"
   ]
  }
